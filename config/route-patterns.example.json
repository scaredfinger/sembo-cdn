{
  "patterns": [
    {
      "regex": "^/hotels/([^/]+)$",
      "name": "hotels/[name]"
    },
    {
      "regex": "^/hotels/([^/]+)/rooms$",
      "name": "hotels/[name]/rooms"
    },
    {
      "regex": "^/hotels/([^/]+)/rooms/([^/]+)$",
      "name": "hotels/[name]/rooms/[id]"
    },
    {
      "regex": "^/api/v(\\d+)/",
      "name": "api/v[version]"
    },
    {
      "regex": "^/users/([^/]+)$",
      "name": "users/[id]"
    },
    {
      "regex": "^/search\\?",
      "name": "search"
    },
    {
      "regex": "^/products/([^/]+)$",
      "name": "products/[id]"
    },
    {
      "regex": "^/categories/([^/]+)/products$",
      "name": "categories/[name]/products"
    },
    {
      "regex": "^/admin/",
      "name": "admin"
    },
    {
      "regex": "^/health$",
      "name": "health"
    }
  ],
  "fallback": "unknown"
}
